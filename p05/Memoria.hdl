CHIP Memoria {
    IN in[16], load, address[15];
    OUT out[16];

    PARTS:
	DMux4Way(in=load,select1=address[13],select2=address[14],OutA=ram,OutB=rram,OutC=scrn,OutD=keybd);
	Or(in1=ram,in2=rram,out=rr);
	RAM16K(in=in,load=rr,address=address[0..13],out=r);
	Screen(in=in,load=scrn,address=address[0..12],out=sc);
	Keyboard(out=k);
	Mux4Way16(a=r,b=r,c=sc,d=k,sel=address[13..14],out=out);

}